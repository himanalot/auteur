<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest Version="11.0" ExtensionBundleId="com.bymaximise.aeextension" ExtensionBundleVersion="1.0.0"
                  ExtensionBundleName="Maximise AE Tools" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ExtensionList>
    <Extension Id="com.bymaximise.aeextension.panel" Version="1.0.0"/>
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="AEFT" Version="[22.0,99.9]"/>
    </HostList>
    <LocaleList>
      <Locale Code="All"/>
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="12.0"/>
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.bymaximise.aeextension.panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./index.html</MainPath>
          <ScriptPath>./jsx/hostscript.jsx</ScriptPath>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>Maximise AE Tools</Menu>
          <Geometry>
            <Size>
              <Height>600</Height>
              <Width>280</Width>
            </Size>
            <MinSize>
              <Height>300</Height>
              <Width>250</Width>
            </MinSize>
            <MaxSize>
              <Height>1200</Height>
              <Width>400</Width>
            </MaxSize>
          </Geometry>
          <Icons>
            <Icon Type="Normal">./icons/icon-normal.png</Icon>
            <Icon Type="RollOver">./icons/icon-rollover.png</Icon>
            <Icon Type="DarkNormal">./icons/icon-dark-normal.png</Icon>
            <Icon Type="DarkRollOver">./icons/icon-dark-rollover.png</Icon>
          </Icons>
        </UI>
        <CEFCommandLine>
          <Parameter>--enable-nodejs</Parameter>
          <Parameter>--mixed-context</Parameter>
          <Parameter>--disable-web-security</Parameter>
          <Parameter>--allow-file-access-from-files</Parameter>
          <Parameter>--allow-running-insecure-content</Parameter>
        </CEFCommandLine>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest> 